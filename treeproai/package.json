{
  "name": "treeproai-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "db:generate": "pnpm --filter @treeproai/db db:generate",
    "db:migrate": "pnpm --filter @treeproai/db db:migrate",
    "db:seed": "pnpm --filter @treeproai/db db:seed"
  },
  "devDependencies": {
    "turbo": "^2.0.4"
  },
  "packageManager": "pnpm@9.1.0"
}