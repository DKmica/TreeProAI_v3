{
  "name": "treeproai-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:e2e": "pnpm --filter web test:e2e",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "db:generate": "pnpm --filter @treeproai/db db:generate",
    "db:migrate": "pnpm --filter @treeproai/db db:migrate",
    "db:seed": "pnpm --filter @treeproai/db db:seed"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "prettier": "^3.3.2",
    "turbo": "^2.0.4",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.1.4"
}